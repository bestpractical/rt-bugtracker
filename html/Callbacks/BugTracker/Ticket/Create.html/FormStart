<%args>
$ARGSRef
</%args>
<%init>
my $Queue = $ARGSRef->{Queue}
    or return;

my $QueueObj = RT::Queue->new($session{CurrentUser});
$QueueObj->Load($Queue);

if ( $QueueObj->Disabled ) {
    $m->comp('/Dist/Elements/ErrorDisabled', Queue => $QueueObj );
    $m->abort;
}
</%init>

<& /Dist/Elements/ShowNotes, Queue => $QueueObj &>

